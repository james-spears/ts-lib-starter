{
  "name": "@james-spears/ts-lib-starter",
  "version": "0.0.6",
  "description": "TypeScript starter template.",
  "main": "dist/index.cjs",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.cjs",
    "watch": "nodemon -L --exec \"npm run build && npm run start\" --watch ./src/**",
    "doc": "typedoc ./src/index.ts",
    "test": "jest --detectOpenHandles --coverage",
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c rollup.config.js",
    "format": "prettier . --write",
    "lint": "eslint -c eslint.config.js . --fix",
    "check": "prettier . --check --log-level silent && eslint -c eslint.config.js .",
    "tidy": "npm run format && npm run lint"
  },
  "author": "James W. Spears",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "rollup": "^4.43.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.21.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jameone/ts-lib-starter.git"
  },
  "files": [
    "dist/**"
  ],
  "bugs": {
    "url": "https://github.com/jameone/ts-lib-starter/issues"
  },
  "homepage": "https://github.com/jameone/ts-lib-starter#readme",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "ws": "^8.18.3"
  }
}
